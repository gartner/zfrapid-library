{
  "name": "zfrapid/zfrapid-library",
  "description": "ZF module with a library to be used by ZFrapid applications",
  "license": "The MIT License (MIT)",
  "type": "library",
  "homepage": "https://github.com/zfrapid/zfrapid-library",
  "authors": [
    {
      "name": "Ralf Eggert",
      "email": "ralf@travello.de",
      "homepage": "http://www.travello.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5",
    "zendframework/zend-eventmanager": "^2.5 || ^3.0",
    "zendframework/zend-mvc": "^2.5 || ^3.0",
    "zendframework/zend-view": "~2.8"
  },
  "require-dev": {
    "bjyoungblood/bjy-profiler": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "ZFrapidLibrary\\": "src/"
    }
  },
  "extra": {
    "zf": {
      "component": "ZFrapidLibrary",
      "config-provider": "ZFrapidLibrary\\ConfigProvider"
    }
  }
}
